{
    "name": "caneco/uncommonmark",
    "description": "Some **uncommon** markdown extensions",
    "keywords": ["caneco", "uncommonmark", "commonmark", "markdown"],
    "homepage": "https://github.com/caneco/uncommonmark",
    "license": "MIT",
    "authors": [
        {
            "name": "Caneco",
            "email": "caneco@me.com",
            "role": "Full-Stack Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "league/commonmark": "^2.4"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.20",
        "symfony/var-dumper": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Caneco\\Uncommonmark\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Caneco\\Uncommonmark\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
